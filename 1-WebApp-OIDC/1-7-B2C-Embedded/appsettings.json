{
    "AzureAdB2C": {
        "Instance": "https://login.yourcustomdomain.com", //Must be a custom domain name. For example: https://login.contoso.com
        "ClientId": "<app-client-id>",
        "Domain": "yourtenant.onmicrosoft.com",
        "SignedOutCallbackPath": "/signout/B2C_1A_signup_signin",
        "SignUpSignInPolicyId": "B2C_1A_signup_signin", //Note the policy name starts with B2C_1A, indicating this is a custom policy. Only custom policies can be used with Embedded sign-in.
        "ResetPasswordPolicyId": "B2C_1A_PasswordReset",
        "EditProfilePolicyId": "B2C_1A_ProfileEdit"
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
